{
    "name": "itinerare/mundialis",
    "type": "project",
    "description": "A web app framework for managing story, character, and world information.",
    "license": "Parity-7.0",
    "require": {
        "php": "^8.0",
        "benjamincrozat/laravel-dropbox-driver": "^3.0",
        "doctrine/dbal": "2.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/database": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/http": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/pagination": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/queue": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/routing": "^5.6|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0",
        "intervention/image": "^2.5",
        "laracasts/flash": "^3.2",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^8.40",
        "laravel/helpers": "^1.4",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.0",
        "spatie/laravel-backup": "^6.16"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.5",
        "laravel/sail": "^1.0.1",
        "marcocesarato/php-conventional-changelog": "^1.10",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "sniff": [
            "./vendor/bin/php-cs-fixer fix -vvv --dry-run --show-progress=dots"
        ],
        "lint": [
            "./vendor/bin/php-cs-fixer fix -vvv --show-progress=dots"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.3.4"
}
